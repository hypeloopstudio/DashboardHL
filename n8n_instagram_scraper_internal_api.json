            ]
        },
        {
            "parameters": {
                "jsCode": "// Procesar la respuesta JSON de Instagram\nconst userData = $input.item.json.data.user || {};\n\n// Obtener el id del webhook original\nconst webhookData = $('Webhook').first().json.body;\nconst recordId = webhookData?.id || null;\n\nreturn {\n  json: {\n    id: recordId,\n    username: userData.full_name || userData.username,\n    profile_pic_url: userData.profile_pic_url_hd || userData.profile_pic_url\n  }\n};"
            },
