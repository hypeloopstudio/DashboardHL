                460,
                300
            ]
        },
        {
            "parameters": {
                "jsCode": "// Extraer username\nconst url = $input.item.json.body.instagram_url || '';\nconst match = url.match(/(?:instagram\\.com|instagr\\.am)\\/([A-Za-z0-9_\\.]+)/);\nconst username = match ? match[1] : null;\n\nreturn {\n  json: {\n    username: username\n  }\n};"
            },
            "id": "extract-username",
            "name": "Extract Username",
            "type": "n8n-nodes-base.code",
            "typeVersion": 1,
            "position": [
                680,
                300
            ]
        },
        {
            "parameters": {
                "url": "={{ `https://www.instagram.com/api/v1/users/web_profile_info/?username=${$json.username}` }}",
                "options": {
                    "redirect": {
                        "redirect": {
                            "followRedirects": false
                        }
                    }
                },
                "headerParameters": {
                    "parameters": [
                        {
                            "name": "User-Agent",
                            "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
